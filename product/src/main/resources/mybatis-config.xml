<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 开启驼峰匹配 -->
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <!-- 别名 -->
    <typeAliases>
        <package name="cn.xss.core.pojo"/>
    </typeAliases>
    <plugins>
        <!--自定义分页插件-->
        <plugin interceptor="cn.xss.core.tools.PageHelper"></plugin>
    </plugins>
</configuration>